generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id          Int      @id @default(autoincrement())
  title       String
  content     String
  createdAt   DateTime @default(now())
  countryName String?
  userId      Int
  user        User     @relation(fields: [userId], references: [id])
}

model User {
  id       Int     @id @default(autoincrement())
  name     String?
  image    String
  email    String @unique
  password String
  posts    Post[]
}
